# Temporary SSH config generated by pipeline

Host ssh_connection_target
    HostName localhost
    User donny
    Port 22
    RequestTty force
    ServerAliveCountMax 2
    StrictHostKeyChecking no
    IdentityFile /home/donny/.ssh/id_rsa_no_password
    RemoteCommand mkdir -p /home/donny/workspaces && cd /home/donny/workspaces && bash -l
    ServerAliveInterval 300
