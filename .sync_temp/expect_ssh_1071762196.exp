#!/usr/bin/expect -f
set timeout 30
spawn ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null donny@localhost -p 22
expect {
    "password:" {
        send "43lw9rj2\r"
        interact
    }
    "yes/no" {
        send "yes\r"
        expect "password:"
        send "43lw9rj2\r"
        interact
    }
    timeout {
        exit 1
    }
    eof {
        exit 0
    }
}
